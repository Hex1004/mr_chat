{
    "builds": [{
        "src": "mr_chat/wsgi.py",
        "use": "@vercel/python",
        "config": {
            "maxLambdaSize": "40mb",
            "runtime": "python3.11",
            "installCommand": "apt-get update && apt-get install -y libpq-dev && pip install -r requirements.txt"
        }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "mr_chat/wsgi.py"
        }
    ]
}
